<!DOCTYPE html>
<html lang="en">
<head>
 <title>CSS3 실습 예제</title>
<script>
    function dan(n){
      var m=parseInt(n);
      if(isNaN(m) || m<1 || m>9){
        alert("잘못 입력했어");
        return;
      }
      for(var i=1;i<=9;i++){
        document.write(n+"*"+i+"="+n*i +"<br>");
      }
    }
</script>
</head>
<body>
<script>
    var n=prompt("몇 단?"); //9
    dan(n);
</script>
</body>
</html>
</body>
</html>
