<script>
       var a=new Array();
       a[0]=1.1;
       a[1]=2.2;
       a[2]=3.3;

       var sum=0;
       for(var i=0;i<a.length;i++){
        sum+=a[i];
       }
       document.write(sum/a.length);
       
</script>
