<!DOCTYPE html>
<html lang="en">
<head>
 <title>CSS3 실습 예제</title>
<script>
    //리터럴 표기법
    var n={
    name:"user1",
    id:"123",
    money:5000,
    a:function(){
        return this.money;  //5000
    },
    plus:function(num){
        this.money+=num;    //10000
    },
    minus:function(num){    //6000
        this.money-=num;
    }    
    }

</script>
</head>
<body>
<script>
    n.plus(5000);
    document.write(n.a()+"<br>");
    
    n.minus(4000);
    document.write(n.a()+"<br>")
</script>
</body>
</html>
</body>
</html>
