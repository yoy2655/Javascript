<!DOCTYPE html>
<html lang="en">
<head>
 <title>CSS3 실습 예제</title>
<script>
    function gugu(){
            var a=prompt("몇단?");
            a=parseInt(a);

            for(var i=1; i<10; i++){
             document.write(a+"*"+i+"="+a*i+"<br>");
          }
        }

          gugu();
</script>
</head>
<body>
<script>
    var food=['라면','초밥','가츠동','사케동']
    document.write('<ul>');
    for(var i=0; i<4; ++i)
    document.write('<li>'+food[i]+'</li>');

    document.write('</ul>');
</script>
</body>
</html>
</body>
</html>
